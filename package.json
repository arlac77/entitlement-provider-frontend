{
  "name": "entitlement-provider-frontend",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "description": "web frontend for the entitlement-provider",
  "keywords": [
    "svelte",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "rollup -c rollup.config.mjs && rollup -c",
    "start": "rollup -c -w rollup.config.mjs && rollup -c -w",
    "lint": "npm run lint:css",
    "lint:css": "stylelint ./src/*.css"
  },
  "dependencies": {
    "@kronos-integration/svelte-components": "^2.4.72",
    "mf-styling": "^1.0.14",
    "svelte-command": "^1.1.9",
    "svelte-common": "^4.4.33",
    "svelte-entitlement": "^1.2.19",
    "svelte-guard-history-router": "^4.6.7",
    "svelte-session-manager": "^2.0.10",
    "svelte-websocket-store": "^1.1.24"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-virtual": "^2.1.0",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "npm-pkgbuild": "^10.7.1",
    "postcss": "^8.4.13",
    "postcss-import": "^14.1.0",
    "rollup": "^2.73.0",
    "rollup-plugin-consts": "^1.1.0",
    "rollup-plugin-dev": "^2.0.4",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.2",
    "stylelint": "^14.8.2",
    "stylelint-config-standard": "^25.0.0",
    "svelte": "^3.48.0"
  },
  "optionalDependencies": {
    "mf-hosting": "github:arlac77/mf-hosting"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/entitlement-provider-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/entitlement-provider-frontend/issues"
  },
  "homepage": "https://github.com/arlac77/entitlement-provider-frontend#readme",
  "config": {
    "api": "/services/entitlements/api",
    "api_ws": "wss://mfelten.dynv6.net/services/entitlements/api",
    "base": "/services/entitlements",
    "proxyTarget": "https://mfelten.dynv6.net/",
    "title": "Entitlements"
  },
  "pkgbuild": {
    "content": {
      "${install.dir}": {
        "base": "public"
      },
      "${nginx.sites.dir}${name}.conf": "pkg/nginx.conf"
    },
    "hooks": "pkg/hooks.sh",
    "groups": "web",
    "depends": {
      "entitlement-provider": ">=2.9.69",
      "nginx-mainline": ">=1.21.4"
    },
    "install.dir": "${install.base.dir}entitlement-provider/frontend/"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild --available --verbose"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-nginx",
      "arlac77/template-svelte-app"
    ]
  }
}
